  "scripts": {
    "test": "jest",
    "coverage": "npm run sandbox-manager:coverage",
    "sandbox-manager:install": "cd projects/sandbox-manager && npm install && cd ../..",
    "sandbox-manager:build:local": "cd projects/sandbox-manager && NODE_ENV=local webpack",
    "sandbox-manager:build:test": "cd projects/sandbox-manager && NODE_ENV=test webpack",
    "sandbox-manager:build:prod": "cd projects/sandbox-manager && NODE_ENV=production webpack",
    "sm": "npm run sandbox-manager:start:local",
    "sandbox-manager:start:local": "cd projects/sandbox-manager && npm run start:local",
    "smt": "cd projects/sandbox-manager && npm run start:test",
    "sandbox-manager:test": "NODE_ENV=test mocha --opts mocha.opts",
    "smtw": "npm run sandbox-manager:test:watch",
    "sandbox-manager:test:watch": "NODE_ENV=test mocha --opts mocha.opts --watch",
    "sandbox-manager:test:coverage": "npm run sandbox-manager:clean:tests_reports && NODE_ENV=test nyc --report-dir projects/sandbox-manager/test_reports/coverage mocha --opts mocha.cov.opts --reporter mochawesome --reporter-options reportDir=projects/sandbox-manager/test_reports/tests,reportFilename=index",
    "sandbox-manager:clean:tests_reports": "rm -rf projects/sandbox-manager/test_reports"
  },
  "pre-push": [
    "coverage",
    "sandbox-manager:check:coverage"
  ],
  "nyc": {
    "include": [],
    "exclude": [
      "coverage/*.js",
      "node_modules",
      "postcss.config.js",
      "projects/sandbox-manager/webpack.config.js",
      "projects/sandbox-manager/build/www/*.js",
      "projects/sandbox-manager/build/www/externals/*.js",
      "projects/sandbox-manager/externals/*.js",
      "**/__test__",
      "__storage__"
    ],
    "extension": [
      ".js",
      ".jsx"
    ],
    "reporter": [
      "text-summary",
      "html",
      "clover",
      "json-summary"
    ],
    "sourceMap": true,
    "instrument": true,
    "all": false
  },
